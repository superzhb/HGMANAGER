<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
	<display-name></display-name>
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>

	<servlet>
		<servlet-name>main</servlet-name>
		<servlet-class>com.hengguang.servlet.Main</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>main</servlet-name>
		<url-pattern>/main</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>showmtiedata</servlet-name>
		<servlet-class>com.hengguang.servlet.ShowMTIEDataServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>showmtiedata</servlet-name>
		<url-pattern>/showmtiedata</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>showtiedata</servlet-name>
		<servlet-class>com.hengguang.servlet.ShowTIEDataServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>showtiedata</servlet-name>
		<url-pattern>/showtiedata</url-pattern>
	</servlet-mapping>


	<servlet>
		<servlet-name>detaile</servlet-name>
		<servlet-class>com.hengguang.servlet.DetaileServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>detaile</servlet-name>
		<url-pattern>/detaile</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>uploadfile</servlet-name>
		<servlet-class>com.hengguang.servlet.FileUploadServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>uploadfile</servlet-name>
		<url-pattern>/uploadfile</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>delrecord</servlet-name>
		<servlet-class>com.hengguang.servlet.DelRecordServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>delrecord</servlet-name>
		<url-pattern>/delrecord</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>login</servlet-name>
		<servlet-class>com.hengguang.servlet.LoginServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>

</web-app>
